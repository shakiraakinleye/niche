{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@/app/*": ["./src/app/*"],
      "@/components/*": ["./src/client/components/*"],
      "@/context/*": ["./src/client/context/*"],
      "@/lib/*": ["./src/client/lib/*"],
      "@/types/*": ["./src/client/types/*"],
    },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/**/*",
    "src/server/services/user.service.tsx",
    "src/app/(seller)/dashboard/orders/payouts",
    "src/app/(shopping)/shop/[[...shopName]]"
  ],
  "exclude": ["node_modules"]
}
